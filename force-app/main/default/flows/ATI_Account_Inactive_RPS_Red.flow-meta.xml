            </connector>
            <label>Double Red</label>
        </rules>
        <rules>
            <name>Triple_Red</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Triple Red</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Field_tripleRed</targetReference>
            </connector>
            <label>Triple Red</label>
        </rules>
        <rules>
            <name>Yellow</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Yellow</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Red</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Verified</targetReference>
            </connector>
            <label>Yellow/Red</label>
        </rules>
        <rules>
            <name>Green</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Green</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.ATI_Sales_Verification_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>PASS</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Verification_Status_Green</targetReference>
            </connector>
            <label>Green</label>
        </rules>
        <rules>
            <name>Pending</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Pending</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Pending_Verified_Status_check</targetReference>
            </connector>
            <label>Pending</label>
        </rules>
        <rules>
            <name>No_Match</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.ECUSTOMS__RPS_Status__c</leftValueReference>
